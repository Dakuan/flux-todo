{
  "name": "flux",
  "version": "0.0.1",
  "description": "Sample react coffeescript todo app using flux with gulp build process",
  "main": "src/js/app.js",
  "dependencies": {
    "es6-promise": "^1.0.0",
    "gulp": "^3.8.7",
    "react": "^0.11.1"
  },
  "devDependencies": {
    "browser-sync": "^1.3.6",
    "browserify": "^5.10.0",
    "browserify-shim": "^3.7.0",
    "coffee-reactify": "^0.5.0",
    "coffee-script": "^1.7.1",
    "gulp-autoprefixer": "0.0.8",
    "gulp-changed": "^1.0.0",
    "gulp-coffee": "^2.1.1",
    "gulp-coffeelint": "^0.3.3",
    "gulp-concat": "^2.3.4",
    "gulp-ignore": "^1.2.0",
    "gulp-imagemin": "^1.0.0",
    "gulp-jest": "^0.2.1",
    "gulp-load-plugins": "^0.5.3",
    "gulp-minify-html": "^0.1.4",
    "gulp-notify": "^1.5.0",
    "gulp-prettify": "^0.3.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-ruby-haml": "0.0.3",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-size": "^1.0.0",
    "gulp-strip-debug": "^1.0.0",
    "gulp-util": "^3.0.0",
    "jest-cli": "^0.1.18",
    "karma": "^0.12.21",
    "karma-browserifast": "^0.7.0",
    "karma-chrome-launcher": "^0.1.4",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-coverage": "^0.2.6",
    "karma-firefox-launcher": "^0.1.3",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "lodash": "^2.4.1",
    "path": "^0.4.9",
    "pretty-hrtime": "^0.2.1",
    "react-tools": "^0.11.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.2"
  },
  "jest": {
    "testDirectoryName": "__tests__",
    "scriptPreprocessor": "<rootDir>/coffee-preprocessor.js",
    "testFileExtensions": [
      "coffee",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/lodash",
      "node_modules/es6-promise"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee",
      "litcoffee",
      "coffee.md"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "bower_components"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffee-reactify"
    ]
  },
  "browserify-shim": {},
  "scripts": {
    "test": "jest"
  },
  "author": "vyorkin",
  "license": "MIT"
}
